											</Greater_Or_Equal>
										</Or>
									</Condition>
									<Then>
										<Constant>yellow</Constant>
									</Then>
									<Else>
										<Constant>white</Constant>
									</Else>
								</If>
							</Else>
						</If>
					</Color>
				</Right>
